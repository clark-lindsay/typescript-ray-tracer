  "compilerOptions": {
    "types": ["node", "jest"],
    "target": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true
  },
  "compileOnSave": true
}
